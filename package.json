    "isaacscript-common-node": "^4.1.1",
    "isaacscript-lint": "^7.0.6",
    "isaacscript-spell": "^1.15.2",
    "isaacscript-tsconfig": "^7.0.1",
    "tsx": "^4.19.1",
    "typescript": "5.5.2",
    "typescript-to-lua": "^1.26.2"
  }
